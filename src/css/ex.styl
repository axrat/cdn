@charset "utf-8"
/* CSS Document */

/* Helper*/
.alert{
    background-color:red!important;
    color:white!important;
}
.no{
    text-decoration: line-through;
}
.true{
    color: #3c763d;
    background-color: #dff0d8;
}
.false{
    color: #a94442;
    background-color: #f2dede;
}
.inline_block{display:inline-block;}
.margin_auto{margin: 0 auto;}
.fixed{position:fixed;}
.absolute{position:absolute;}
.relative{position:relative;}
.top{top:0;}
.bottom{bottom:0;}
.left{left:0;}
.right{right:0;}
.vertical_top{vertical-align:top;}
.vertical_middle{vertical-align:middle;}
.vertical_bottom{vertical-align:bottom;}
.text_right{text-align:right;}
.text_left{text-align:left;}
.text_center{text-align:center;}
.crossoff{text-decoration:line-through;}
.bold{font-weight:bold;}
.responsive{max-width:100%;}
.margin{margin:5px;}
.padding{padding:5px;}
.padding_top{padding-top:5px;}
.z_index{z-index:2147483647;}
.color_white{color:white;}
.table_container {
    width: 100%;
    overflow-y: auto;
    _overflow: auto;
    margin: 0 0 1em;
}
.high{
    padding-top: 0;
    margin-top: 0;
}
.low{
    padding-bottom: 0;
    margin-bottom: 0;
}
hr.slash{
    height: 6px;
    background: url("data:image/png;base64,") repeat-x 0 0;
    border: 0;
}
hr.wave{
    height: 6px;
    background: url("data:image/png;base64,") repeat-x 0 0;
    border: 0;
    margin-top: 5px;
    margin-bottom: 8px;
}
/* Effect */
.imgZoom {
    overflow: hidden;
}
.imgZoom img {
    display: block;
    transition-duration: 0.3s;
}
.imgZoom img:hover {
    transform: scale(1.1);
    transition-duration: 0.3s;
}
/* Tree */
.tree{
    font-size:90%;
    overflow: auto;
}
.tree h1 {
    margin: 0;
    line-height: 1.5;
    font-weight: normal;
    font-size: 100%;
}
.tree ul {
    text-align: left;
    display: inline-block;
    margin: 0;
    padding: 0;
    line-height: 1.5;
    list-style: none;
}
.tree li{
    text-align: left;
    white-space: nowrap;
}
.tree ul li {
    margin: 0 0 0 0.5em;
    padding: 0;
    border-left: 1px solid #999;
    zoom: 1;
}
.tree ul li:before {
    margin-right: 0.5em;
    border-bottom: 1px solid #999;
    float: left;
    width: 1em;
    height: 0.75em;
    overflow: hidden;
    content: "";
}
.tree ul li:last-child {
    border: none;
}
.tree ul li:last-child:before {
    border-left: 1px solid #999;
}
/* Wide */
ul.wide_list,
ul.wide_list_border,
ul.wide_list_simple{
    font-size: 90%;
    padding: 0;
    margin: 0 auto;
    color: #808080;
}
ul.wide_list li,
ul.wide_list_border li,
ul.wide_list_simple li{
    display: inline-block;
    list-style-type: none;
    display: -moz-inline-box;
    padding-left:0px;
    padding-right:0px;
    margin-left:0px;
    margin-right:0px;
}
ul.wide_list_border li{
    border-left:1px solid #808080;
    padding-left:5px;
    margin-left:5px;
}
ul.wide_list_simple li:before {
    color: #808080;
    content: " | ";
}
ul.wide_list li:first-child,
ul.wide_list_border li:first-child,
ul.wide_list_simple li:first-child{
    padding-left:0px;
    margin-left:0px;
}
ul.wide_list_border li:first-child{
    border-left:none;
}
ul.wide_list_simple li:first-child:before {
    content: normal;
}
/* Button */
.btn{
    font-size:12px;
    font-weight: bold;
    margin: auto 2px;
    padding: 2px 5px 2px 4px;
    color: #333;
    text-decoration: none;
    white-space: nowrap;
    cursor: pointer;
    border-radius: 3px;
    border: 1px solid #d4d4d4;
    border-bottom-color: #bcbcbc;
    background-color: #e6e6e6;
    background-repeat: no-repeat;
    line-height: 20px;
}
/* Hover */
.hover:hover{
    background: -moz-linear-gradient(top,  rgba(0,0,0,0) 0%, rgba(0,0,0,0.2) 20%);
    background: -webkit-linear-gradient(top,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.2) 20%);
    background: linear-gradient(to bottom,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.2) 20%);
}
/* Fade */
.fadeinout{
    opacity: 0;
    transition: opacity 0.5s ease-out;
}
.fadeinout:hover {
    opacity: 1;
}
/* Shake */
.shake-hover:hover {
    display: inline-block;
    animation: gakuburu .1s  infinite;
}
@keyframes gakuburu {
    0% {transform: translate(0px, 0px) rotateZ(0deg)}
    25% {transform: translate(2px, 2px) rotateZ(1deg)}
    50% {transform: translate(0px, 2px) rotateZ(0deg)}
    75% {transform: translate(2px, 0px) rotateZ(-1deg)}
    100% {transform: translate(0px, 0px) rotateZ(0deg)}
}
/* BackgroundVideo */
.bg_video{
    opacity:1.0;
    z-index:-100;
    min-width: 100%;
    min-height: 100%;
    position: fixed;
}
.marquee {
    min-width: 240px;
    max-width: 980px;
    margin: 0 auto;
    overflow: hidden;
    white-space: nowrap;
    box-sizing: border-box;
    animation: marquee 15s linear infinite;
}
.marquee:hover {
    animation-play-state: paused
}
@keyframes marquee {
    100% { text-indent: -15em }
    0%   { text-indent: 70em }
}
/* BackgroundGridCSS */
.bg_grid{
  background-color: #fbfbfb;
  background-image:
  linear-gradient(rgba(146,146,146,.5) 1px, transparent 1px),
  linear-gradient(90deg, rgba(146,146,146,.5) 1px, transparent 1px),
  linear-gradient(rgba(180,180,180,.3) 1px, transparent 1px),
  linear-gradient(90deg, rgba(180,180,180,.3) 1px, transparent 1px);
  background-size:100px 100px, 100px 100px, 20px 20px, 20px 20px;
  background-position:-2px -2px;
}
